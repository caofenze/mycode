.mod-slides{
	display: none ;
}
#buyNav{
	float: left;
}
.mod-buy-banner{
	height: 360px;
}
.mod-sec-kill{
	border: 1px solid #e6e6e6;
	width: 300px;
	height: 360px;
	float: right;
	display: inline-block;
	position: relative;
	text-align: center;
}
.mod-sec-kill img{
	width: 270px;
	height: 190px;
}
.mod-sec-kill .staff-cut {
	background: url(../img/bargin.png) no-repeat;
	display: block;
	width: 80px;
	height: 56px;
	position: absolute;
	right: -6px;
	color: #fff;
}
.mod-sec-kill .staff-cut .cut-num {
	font-size: 35px;
	line-height: 54px;
	position: relative;
}
.mod-sec-kill .staff-cut .cut-percent {
	float: right;
	position: relative;
	font-size: 23px;
	left: -8px;
	top: 7px;
}
.mod-sec-kill .staff-cut .cut-on {
	position: absolute;
	right: 6px;
	top: 33px;
	font-size: 10px;
}
.mod-sec-kill .m-mtl {
	margin-top: 5px;
}
.mod-sec-kill .p-wraper {
	width: 260px;
	height: 30px;
	line-height: 15px;
	text-align: center;
	margin: 0 auto;
}
.mod-sec-kill .staff-name {
	color: #353636;
	font-size: 16px;
}
.mod-sec-kill .staff-pro {
	color: #a4a4a4;
	font-size: 14px;
}
.mod-sec-kill .staff-pro {
	color: #a4a4a4;
	font-size: 14px;
}
.mod-sec-kill .staff-origin {
	color: #bfbfbf;
	font-size: 14px;
	text-decoration: line-through;
}
.mod-sec-kill .staff-bargin {
	color: #fc2b3a;
	font-size: 22px;
}
.mod-sec-kill .staff-info {
	display: block;
	text-align: center;
}
.mod-sec-kill .m-mbl {
	margin-bottom: 5px;
}
.mod-sec-kill .sec-timer{
	background: #f6f6f6;
	border-top: 1px solid #e6e6e6;
	height: 48px;
}
.mod-sec-kill .sec-timer .sec-del{
	display: inline-block;
}
.mod-sec-kill .timer-detail .time{
	float: left;
	display: block;
	width: 30px;
	height: 30px;
	line-height: 30px;
	font-size: 16px;
	text-align: center;
	color: #fff;
	margin: 8px 7px -10px;
	background: #4ca6ff;
}
.mod-sec-kill .timer-detail .dot{
	float: left;
	position: relative;
	top: 16px;
}
.mod-sec-kill .img-icon{
	background: url(../img/clock.png) no-repeat;
	width: 20px;
	height: 20px;
	position: relative;
	top: 6px;
	left: -13px;
}
.mod-sec-kill .sec-label-shit{
	top: -77%;
	position: relative;
	background: black;
	padding: 15px;
	border-radius: 4px;
	border: 3px solid red;
	color: white;
	font-size: 25px;
	background: rgba(0,0,0,.4);
	width: 180px;
	margin: 0 auto;
}